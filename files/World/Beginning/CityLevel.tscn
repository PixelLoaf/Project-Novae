code = "shader_type canvas_item;
render_mode unshaded;

uniform vec4 color_base = vec4(0,0,0,1);
uniform float value = 0.0;

void fragment() {
	float threshold = texture(NORMAL_TEXTURE, UV).r;
	if (value > threshold) {
		COLOR = color_base;
	} else {
		COLOR = texture(TEXTURE, UV);
	}
}"

