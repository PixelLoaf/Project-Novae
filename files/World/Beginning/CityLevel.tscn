[sub_resource type="Shader" id=1]

code = "shader_type canvas_item;
render_mode unshaded;

void fragment() {
	vec2 my_uv = UV + vec2(sin((TIME+UV.y*6.0)*1.0)*0.05, 0);
	COLOR = texture(TEXTURE, my_uv);
	vec4 one4 = vec4(1, 1, 1, 1);
	float hue = mod(TIME + UV.y * 5.0, 3.0);
	vec4 huevalue = vec4(0, 0, 0, 0);
	if (hue < 1.0) { // red
		huevalue.r = 1.0 - abs(hue - 0.0);
	}
	if (hue > 2.0) { // red
		huevalue.r = 1.0 - abs(hue - 3.0);
	}
	if (hue < 2.0) { // green
		huevalue.g = 1.0 - abs(hue - 1.0);
	}
	if (hue > 1.0) { // blue
		huevalue.b = 1.0 - abs(hue - 2.0);
	}
	
	COLOR = one4 - ((one4 - COLOR * COLOR) * (one4 - huevalue * 0.5));
}
"

